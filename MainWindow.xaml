                                                     CanDragEndPoint="False" EndArrow="Filled90" EndArrowSize="10,10"
                                                     EndPoint="590,265" Points="(Empty)" StrokeId="Black" 
                                                     TemplateName="InterfaceConnector" Type="Straight"/>
